{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_APP_URL",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "VERCEL_KV_URL",
    "VERCEL_KV_REST_API_URL",
    "VERCEL_KV_REST_API_TOKEN",
    "VERCEL_KV_REST_API_READ_ONLY_TOKEN",
    "EDGE_CONFIG",
    "NEXT_PUBLIC_COUNTRY_STATE_CITY",
    "COUNTRY_STATE_CITY",
    "STRIPE_SECRET_KEY",
    "STRIPE_PUBLISHABLE_KEY",
    "STRIPE_EXPERIENCED_PRICE_ID",
    "STRIPE_JUNIOR_PRICE_ID",
    "STRIPE_LAYOFFED_PRICE_ID",
    "STRIPE_CUST0M_PRICE_ID",
    "STRIPE_WEBHOOK_SECRET",
    "RESEND_API_KEY",
    "UPLOADTHING_SECRET",
    "UPLOADTHING_APP_ID",
    "FRONTEND_DEV_URL",
    "FRONTEND_PROD_URL",
    "BACKEND_PROD_URL",
    "BACKEND_DEV_URL",
    "BACKEND_PORT",
    "JWT_SECRET",
    "DATABASE_URL",
    "DATABASE_URL_REPLICA_1",
    "REDIS_URL",
    "AWS_SES_REGION",
    "EMAIL_SERVER_HOST",
    "EMAIL_SERVER_PORT",
    "EMAIL_SERVER_USER",
    "EMAIL_SERVER_PASSWORD",
    "EMAIL_FROM",
    "AWS_S3_REGION",
    "AWS_S3_ACCESS_KEY_ID",
    "AWS_S3_SECRECT_ACCESS_KEY",
    "AWS_S3_BUCKET_NAME",
    "IS_LOCAL",
    "GRAPHQL_API_URL",
    "SECRET_KEY",
    "SECRET_KEY_VAR",
    "CACHE_DATABASE_URL"
  ],
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_APP_URL",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "NEXTAUTH_SECRET",
        "NEXTAUTH_URL",
        "VERCEL_KV_URL",
        "VERCEL_KV_REST_API_URL",
        "VERCEL_KV_REST_API_TOKEN",
        "VERCEL_KV_REST_API_READ_ONLY_TOKEN",
        "EDGE_CONFIG",
        "COUNTRY_STATE_CITY",
        "STRIPE_SECRET_KEY",
        "STRIPE_PUBLISHABLE_KEY",
        "STRIPE_EXPERIENCED_PRICE_ID",
        "STRIPE_JUNIOR_PRICE_ID",
        "STRIPE_LAYOFFED_PRICE_ID",
        "STRIPE_CUST0M_PRICE_ID",
        "STRIPE_WEBHOOK_SECRET",
        "RESEND_API_KEY",
        "UPLOADTHING_SECRET",
        "UPLOADTHING_APP_ID",
        "FRONTEND_DEV_URL",
        "FRONTEND_PROD_URL",
        "BACKEND_PROD_URL",
        "BACKEND_DEV_URL",
        "BACKEND_PORT",
        "JWT_SECRET",
        "DATABASE_URL",
        "DATABASE_URL_REPLICA_1",
        "REDIS_URL",
        "AWS_SES_REGION",
        "EMAIL_SERVER_HOST",
        "EMAIL_SERVER_PORT",
        "EMAIL_SERVER_USER",
        "EMAIL_SERVER_PASSWORD",
        "EMAIL_FROM",
        "AWS_S3_REGION",
        "AWS_S3_ACCESS_KEY_ID",
        "AWS_S3_SECRECT_ACCESS_KEY",
        "AWS_S3_BUCKET_NAME",
        "IS_LOCAL",
        "GRAPHQL_API_URL",
        "SECRET_KEY",
        "SECRET_KEY_VAR",
        "CACHE_DATABASE_URL"
      ],
      "outputs": ["api/**", ".next/**", "!.next/cache/**"]
    },
    "db:generate": {
      "cache": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "format": {
      "cache": false,
      "outputs": []
    },
    "format:check": {
      "cache": false,
      "outputs": []
    },
    "test": {},
    "test:watch": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env"]
    },
    "build:api": {
      "dependsOn": ["^postinstall"],
      "outputs": ["dist/**"]
    }
  }
}
